<style>
    table,
    tb,
    td {
        border: 1px solid black;
    }
</style>
<h3>My Workout Plan</h3>
</form>

<table>
    <tr>
        <th>Bodytype</th>
        <th>BodyPart</th>
        <th>exercise</th>
        <th>reps</th>
        <th>sets</th>
        <th>equipment</th>
    </tr>

    <% workout.forEach((workout)=> { %>
        <tr>
            <td><%= workout.My_bodyType%> </td>
            <td><%= workout.My_BodyPart%> </td>
            <td><%= workout.My_exercise%> </td>
            <td><%= workout.My_reps%> </td>
            <td><%= workout.My_sets%> </td>
            <td><%= workout.My_equipment%> </td>
            <td><a href="/delete" >drop workout</a></td>
        </tr>
        <% }) %>

</table>
