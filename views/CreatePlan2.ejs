</style>
<h3>search result</h3>
</form>

<table>
    <tr>
        <th>Bodytype</th>
        <th>BodyPart</th>
        <th>exercise</th>
        <th>reps</th>
        <th>sets</th>
        <th>equipment</th>
    </tr>

    <% workout.forEach((workout)=> { %>
        <tr>
            <td><%= workout.bodyType%> </td>
            <td><%= workout.BodyPart%> </td>
            <td><%= workout.exercise%> </td>
            <td><%= workout.reps%> </td>
            <td><%= workout._sets%> </td>
            <td><%= workout.equipment%> </td>
            <td><a href="/addExercise/<%= workout.exercise %>" >add to workout plan</a></td>
        </tr>
        <% }) %>

</table>
